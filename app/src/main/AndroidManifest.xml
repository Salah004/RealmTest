<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.salah.realmtest">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.PersonsActivity" />
        <activity android:name=".activities.OfferActivity" />
        <activity android:name=".activities.AbonnementActivity" />
        <activity android:name=".activities.PersonAbonnementsListActivity" />
        <activity android:name=".activities.OfferAbonnementsListActivity"></activity>
    </application>

</manifest>